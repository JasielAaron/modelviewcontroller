<h1>Tech blog</h1>
{{#each blogs as |blog| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2></h2>
      <a href="/project/{{blog.id}}">{{blog.title}}</a>
    </h2>
    <p>Created by {{blog.user.name}} on {{blog.create_on_date}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{blog.content}}
    </p>
  </div>
</div>
{{/each}}